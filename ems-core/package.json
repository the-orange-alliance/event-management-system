{
  "name": "ems-core",
  "version": "2.17.0",
  "private": true,
  "main": "build/electron.js",
  "dependencies": {
    "@the-orange-alliance/lib-ems": "^0.9.1-a",
    "axios": "^0.21.1",
    "dotenv": "^8.0.0",
    "local-ipv4-address": "0.0.2",
    "moment": "^2.29.1",
    "opn": "^6.0.0",
    "pm2": "github:Unitech/pm2#development",
    "react": "^16.8.6",
    "react-datepicker": "^2.10.1",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-scripts": "^4.0.3",
    "react-toastify": "^5.1.0",
    "redux": "^4.0.1",
    "semantic-ui-offline": "^2.4.1",
    "semantic-ui-react": "^0.87.1",
    "socket.io-client": "^2.2.0",
    "winston": "^3.2.1"
  },
  "scripts": {
    "start": "concurrently --kill-others \"npm run desktop\" \"npm run react\"",
    "desktop": "tsc -p tsconfig.electron.json --skipLibCheck && electron .",
    "electron": "electron .",
    "react": "react-app-rewired start",
    "build": "react-app-rewired build",
    "postbuild": "tsc -p tsconfig.electron.json --skipLibCheck",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "postinstall": "electron-rebuild",
    "update-lib-ems": "npm i @the-orange-alliance/lib-ems@latest"
  },
  "devDependencies": {
    "@types/node": "^15.12.5",
    "@types/react": "^16.8.16",
    "@types/react-datepicker": "^2.3.0",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.0.8",
    "@types/react-toastify": "^4.0.2",
    "@types/socket.io-client": "^1.4.32",
    "concurrently": "^4.1.0",
    "electron": "^6.0.12",
    "electron-debug": "^2.2.0",
    "electron-rebuild": "^2.3.5",
    "react-app-rewired": "^2.1.8",
    "typescript": "^4.3.4"
  },
  "homepage": "./",
  "proxy": "http://localhost:4200",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  }
}
